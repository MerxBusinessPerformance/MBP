            <!--<t t-esc="item.string" />-->
            <!--</div>-->
            <!--</t>-->
            <!--</div>-->
            <!--</div>-->
            <!--</t>-->
        </div>
    </t>
    <t t-name="ViewStudio.SearchView">
        <div class="wSearchView">
            <div class="wSVCon">
                <div class="wAuto" name="automation">
                    <div class="wH"><i class="fa fa-magic" />Automation Fields</div>
                    <div class="wACon"></div>
                </div>
                <div class="wFilter" name="filters">
                    <div class="wH"><i class="fa fa-filter" />Filters</div>
                    <div class="wFCon"></div>
                </div>
                <div class="wGroup" name="groups">
                    <div class="wH"><i class="fa fa-bars" />Groups</div>
                    <div class="wGCon"></div>
                </div>
            </div>
        </div>
