        <span>New Column</span>
    </t>
    <t t-name="Studio.virtual.field">
        <span>New Field</span>
    </t>
    <t t-name="ViewStudio.View.TextVirtual">
        <div class="wTextVirtual">
            <p>Text</p>
        </div>
    </t>
    <t t-name="ViewStudio.View.OtherVirtual">
        <div class="wOtherVirtual">
            <p><t t-esc="label" /></p>
        </div>
